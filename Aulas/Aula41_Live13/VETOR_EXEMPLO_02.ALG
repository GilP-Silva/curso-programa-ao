Algoritmo "Vetor_Exemplo_02"

Var

   vet: vetor [1..5] de inteiro
   i : inteiro

Inicio
      //manipulando valores dos
      //elementos manualmente
      vet[1] <- -10
      vet[2] <- 52
      vet[3] <- vet[1] + 63
      vet[4] <- vet[1] - vet[2]
      vet[5] <- 185

      //saída para o vetor
      para i de 1 ate 5 faca
         escreval(vet[i])
      fimpara

Fimalgoritmo

