Algoritmo "Tabuada"

Var

    i, j : inteiro

Inicio

      //inicializando
      i <- 1
      
      enquanto i < 11 faça
      
        para j de 1 até 10 faça
          escreval(i, " X ", j, " = ", i*j)
        fimpara
        escreval()
        //incremento do enquanto
        i <- i + 1
      fimenquanto

Fimalgoritmo


