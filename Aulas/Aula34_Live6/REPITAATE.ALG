Algoritmo "RepitaAte"

Var

   j, x: inteiro

Inicio

   j <- 1
   enquanto j <= 10 faca
      escreva (j:3)
      j <- j + 1
   fimenquanto
    escreval()
   j <- 1
   repita
      escreva (j:3)
      j <- j + 1
   ate j > 10
   
   x <- 0
     escreval()
   repita
      x <- x + 1
      escreva (x:3)
      se x = 10 entao
        // interrompa
      fimse
   ate falso

Fimalgoritmo



