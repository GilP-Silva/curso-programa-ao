Algoritmo "Ex02_Lista03"

Var

    i, soma : inteiro

Inicio
     //inicialização
     i <- 1
     
      //parada
      enquanto i <= 100 faça
      
          soma <- soma + i
          escreval("Soma = ", soma)
          //incremento
          i <- i + 1
      fimenquanto

      escreval("Soma final")
      escreval("Soma = ", soma)
      
Fimalgoritmo