algoritmo "soma"

var

//declaração de variáveis
   A : inteiro
   B, C : inteiro
   
   
inicio

      //comando de saída
      escreva("Digite um numero: ")
      //comando para entrada de dados
      leia(A)
      
      escreva("Digite outro numero: ")
      leia(B)
      //processamento
      
      C <- A + B
      
      //saida
      escreva("Soma = ", C)

fimalgoritmo